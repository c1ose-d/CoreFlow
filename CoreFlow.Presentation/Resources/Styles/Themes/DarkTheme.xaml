    <LinearGradientBrush
        x:Key="ControlElevationBorderBrush"
        EndPoint="0.5,1"
        StartPoint="0.5,0" >
        <LinearGradientBrush.GradientStops>
            <GradientStop
                Color="#16FFFFFF"
                Offset="0.91" />
            <GradientStop
                Color="#11FFFFFF"
                Offset="1.0" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
