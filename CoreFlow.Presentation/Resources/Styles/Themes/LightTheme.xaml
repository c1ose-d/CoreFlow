        <LinearGradientBrush.GradientStops>
            <GradientStop
                Offset="0.91"
                Color="#14FFFFFF" />
            <GradientStop
                Offset="1.0"
                Color="#66000000" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
