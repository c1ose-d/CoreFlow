    <!--Control Elevation-->
    <LinearGradientBrush
        x:Key="ControlElevationBorderBrush"
        EndPoint="0.5,1"
        StartPoint="0.5,0" >
        <LinearGradientBrush.GradientStops>
            <GradientStop
                Offset="0.91"
                Color="#0F000000" />
            <GradientStop
                Offset="1.0"
                Color="#29000000" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush
        x:Key="CircleElevationBorderBrush"
        EndPoint="0.5,1"
        StartPoint="0.5,0" >
        <LinearGradientBrush.GradientStops>
            <GradientStop
                Offset="0.50"
                Color="#0F000000" />
            <GradientStop
                Offset="0.95"
                Color="#29000000" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush
        x:Key="TextControlElevationBorderBrush"
        EndPoint="0.5,1"
        StartPoint="0.5,0" >
        <LinearGradientBrush.GradientStops>
            <GradientStop
                Offset="1.0"
                Color="#0F000000" />
            <GradientStop
                Offset="1.0"
                Color="#72000000" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <LinearGradientBrush
        x:Key="TextControlElevationBorderFocusedBrush"
